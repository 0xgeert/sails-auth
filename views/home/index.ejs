<!-- Default home page -->
<!-- <link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,700italic,400,300,700' rel='stylesheet'/> -->
<link href='http://sailsjs.org/styles/fonts.css' rel='stylesheet'/>
<link href='styles/main.css' rel='stylesheet'/>
<!--[if IE 7]>
<link href='styles/ie7.css' rel='stylesheet'/>
<![endif]-->

<div class="container clearfix">
    <div class="main">

        <span style="color: red" ><%- req.flash('message') %></span>

        <h1 id="main-title">Hi User,</h1>
        <p>You've successfully logged in.</p>
        <a class="btn" href="/logout">Logout</a>

        <h2>User Model</h2>
        <table class="table">
            <% for(key in user.toJSON() ) { %>
            <tr>
                <td><%= key %>: </td>
                <td class="boxy"><%= user[key] %></td>
            </tr>
            <% } %>
        </table>
    </div>
</div>
